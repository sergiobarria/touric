    "express-async-handler": "^1.2.0",
    "http-status": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.7.5",
    "morgan": "^1.10.0",
    "slugify": "^1.6.5",
    "validator": "^13.7.0",
    "winston": "^3.8.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
